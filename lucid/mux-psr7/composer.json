{
  "name": "lucid/mux-psr7",
  "license": "MIT",
  "description": "Psr7 router for lucid/mux",
  "keywords": ["http", "prs7", "routing"],
  "authors": [
    {
      "name": "iwyg",
      "email": "mail@thomas-appel.com"
    }
  ],
  "require": {
    "php":"^5.6 || ^7.0",
    "lucid/mux": "dev-develop",
    "psr/http-message": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit":"^5.2",
    "zendframework/zend-diactoros":"^1.3"
  },
  "autoload": {
    "psr-4": {
      "Lucid\\Mux\\Psr7\\":"src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lucid\\Mux\\Psr7\\Tests\\":"tests/",
      "Lucid\\Mux\\Tests\\":"vendor/lucid/mux/tests/"
    }
  },
  "minimum-stability": "dev"
}
