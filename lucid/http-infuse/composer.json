{
  "name": "lucid/http-infuse",
  "description":"Lucid Http Middleware Package", 
  "keywords": ["http", "middleware", "psr-7"],
  "license": "MIT",
  "authors": [
    {
      "name": "iwyg",
      "email": "mail@thomas-appel.com"
    }
  ],
  "require": {
    "php":">=5.6.0",
    "psr/http-message":"1.0.*@dev"
  },
  "require-dev": {
    "phpunit/phpunit":"^5.2",
    "sebastian/peek-and-poke": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Lucid\\Http\\Infuse\\":"src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lucid\\Http\\Infuse\\Tests\\":"tests/"
    }
  },
  "minimum-stability": "dev",
  "archive": {
    "exclude": [
      ".travis.yml", 
      "phpunit.xml.dist", 
      "tests"
    ]
  }
}
